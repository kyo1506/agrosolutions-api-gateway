{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    }
  },
  "AllowedHosts": "*",
  "RateLimiting": {
    "EnableRateLimiting": true,
    "DefaultLimit": 100,
    "DefaultPeriodInSeconds": 60
  },
  "Jwt": {
    "Issuer": "AgroSolutions",
    "Audience": "AgroSolutions.Services",
    "SecretKey": "YourSuperSecretKeyHere_ChangeInProduction_AtLeast32Characters!"
  }
}
