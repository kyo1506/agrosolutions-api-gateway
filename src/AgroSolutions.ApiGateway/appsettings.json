{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    }
  },
  "AllowedHosts": "*",
  "RateLimiting": {
    "EnableRateLimiting": true,
    "DefaultLimit": 100,
    "DefaultPeriodInSeconds": 60
  },
  "Jwt": {
    "Authority": "http://keycloak:8080/realms/agrosolutions",
    "Audience": "agrosolutions-api"
  }
}
